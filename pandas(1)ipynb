   "source": [
    "#condition ke hisab se\n",
    "\n",
    "def cal_tax(bill):\n",
    "    if(bill<0):\n",
    "        return bill*.05\n",
    "    elif(bill>=10 and bill<=20):\n",
    "        return bill*.10\n",
    "    else:\n",
    "        return bill*.15\n",
    "\n",
    "    \n",
    "cal_tax(10)"
   ]
  },
