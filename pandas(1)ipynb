   "source": [
    "# How can we filter rows based on given conditions\n",
    "\n",
    "# df['col_label']\n",
    "# df[['col_label1','col_label2','col_label3']]\n",
    "\n",
    "# df[condition]\n",
    "\n",
    "#df[(condition1) &(condition2) | ]"
   ]
  },
  {
   "cell_type": "code",
